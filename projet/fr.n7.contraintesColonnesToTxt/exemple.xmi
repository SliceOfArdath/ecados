<?xml version="1.0" encoding="UTF-8"?>
<cONTRAINTES:EnsembleContraintes
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:cONTRAINTES="http://www.n7.fr/CONTRAINTES"
    xsi:schemaLocation="http://www.n7.fr/CONTRAINTES CONTRAINTES.ecore">
  <contraintes
      name="Feur">
    <colonnes
        name="Age"/>
    <colonnes
        name="Taille"/>
    <expression
        xsi:type="cONTRAINTES:OperationBinaire"
        operande="ou">
      <contrainteGauche
          xsi:type="cONTRAINTES:Comparaison"
          comparateur=">"/>
      <contrainteDroite
          xsi:type="cONTRAINTES:Comparaison"
          comparateur="="
          constante="-1.0"/>
    </expression>
  </contraintes>
</cONTRAINTES:EnsembleContraintes>
